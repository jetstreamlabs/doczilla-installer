{
  "name": "doczilla/installer",
  "description": "Command line installer for Doczilla documentation application.",
  "keywords": [
    "doczilla",
    "installer"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "secondman",
      "email": "secondman@jetstreamlabs.com"
    }
  ],
  "require": {
    "php": "^8.2",
    "illuminate/filesystem": "^10.38",
    "illuminate/support": "^10.38",
    "laravel/prompts": "^0.1",
    "symfony/console": "^6.0",
    "symfony/process": "^7.0"
  },
  "require-dev": {
    "jetstreamlabs/pinte": "^1.1",
    "pestphp/pest": "^2.28",
    "phpstan/phpstan": "^1.10",
    "symfony/var-dumper": "^7.0"
  },
  "bin": [
    "bin/doczilla"
  ],
  "autoload": {
    "psr-4": {
      "Doczilla\\Installer\\Console\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
